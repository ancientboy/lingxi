{
  "meta": { "lastTouchedVersion": "2026.2.17" },
  "env": {
    "ZHIPU_API_KEY": "ZHIPU_API_KEY_PLACEHOLDER",
    "DASHSCOPE_API_KEY": "DASHSCOPE_API_KEY_PLACEHOLDER"
  },
  "auth": {
    "profiles": {
      "alibaba-cloud:default": { "provider": "alibaba-cloud", "mode": "api_key" },
      "zhipu:default": { "provider": "zhipu", "mode": "api_key" }
    }
  },
  "models": {
    "mode": "merge",
    "providers": {
      "alibaba-cloud": {
        "baseUrl": "https://coding.dashscope.aliyuncs.com/v1",
        "api": "openai-completions",
        "models": [
          { "id": "qwen3-max-2026-01-23", "name": "qwen3-max", "contextWindow": 262144, "maxTokens": 65536 }
        ]
      },
      "zhipu": {
        "baseUrl": "https://open.bigmodel.cn/api/coding/paas/v4",
        "api": "openai-completions",
        "authHeader": true,
        "models": [
          { "id": "glm-5", "name": "GLM-5", "contextWindow": 200000, "maxTokens": 8192 },
          { "id": "glm-4-air", "name": "GLM-4-Air", "contextWindow": 128000, "maxTokens": 4096 }
        ]
      }
    }
  },
  "agents": {
    "defaults": { 
      "model": { "primary": "zhipu/glm-5" }, 
      "workspace": "~/.openclaw/workspace" 
    },
    "list": [
      { "id": "main", "default": true, "name": "灵犀", "agentDir": "~/.openclaw/agents/main/agent", "subagents": { "allowAgents": ["coder", "ops", "inventor", "pm", "noter", "media", "smart"] } },
      { "id": "coder", "name": "云溪", "agentDir": "~/.openclaw/agents/coder/agent" },
      { "id": "ops", "name": "若曦", "agentDir": "~/.openclaw/agents/ops/agent" },
      { "id": "inventor", "name": "紫萱", "agentDir": "~/.openclaw/agents/inventor/agent" },
      { "id": "pm", "name": "梓萱", "agentDir": "~/.openclaw/agents/pm/agent" },
      { "id": "noter", "name": "晓琳", "agentDir": "~/.openclaw/agents/noter/agent" },
      { "id": "media", "name": "音韵", "agentDir": "~/.openclaw/agents/media/agent" },
      { "id": "smart", "name": "智家", "agentDir": "~/.openclaw/agents/smart/agent" }
    ]
  },
  "tools": {
    "agentToAgent": { "enabled": true, "allow": ["main", "coder", "ops", "inventor", "pm", "noter", "media", "smart"] },
    "subagents": { 
      "tools": { 
        "allow": ["group:fs", "group:runtime", "group:sessions", "group:memory", "web_search", "web_fetch"] 
      } 
    }
  },
  "hooks": {
    "enabled": true,
    "token": "GATEWAY_TOKEN_PLACEHOLDER"
  },
  "gateway": {
    "port": 18789,
    "mode": "local",
    "bind": "lan",
    "controlUi": {
      "enabled": true,
      "basePath": "SESSION_ID_PLACEHOLDER",
      "allowedOrigins": ["*"],
      "allowInsecureAuth": true,
      "dangerouslyDisableDeviceAuth": true
    },
    "auth": { "mode": "token", "token": "GATEWAY_TOKEN_PLACEHOLDER" }
  },
  "plugins": {
    "entries": {
      "feishu": {
        "enabled": false,
        "config": {
          "appId": "FEISHU_APP_ID_PLACEHOLDER",
          "appSecret": "FEISHU_APP_SECRET_PLACEHOLDER"
        }
      }
    }
  }
}
